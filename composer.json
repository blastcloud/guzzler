{
    "name": "blastcloud/guzzler",
    "description": "Supercharge your unit tests that use Guzzle with a mock-like syntax.",
    "keywords": ["guzzle", "phpunit", "testing", "php", "mock"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adam Kelso",
            "email": "kelso.adam@gmail.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpunit/phpunit": ">=7.0",
        "guzzlehttp/guzzle": ">=6.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "BlastCloud\\Guzzler\\": "src/",
            "tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4"
    }
}
